{
	"manifest_version": 3,
	"name": "bkmrks",
	"description": "A simple New-Tab-Page for displaying all your bookmarks on one page.",
	"version": "2.0.0",

	"chrome_url_overrides": {
		"newtab": "bkmrks.html"
	},

	"permissions": ["bookmarks", "storage"],

	"icons": {
		"128": "icon.png"
	},

	"options_ui": {
		"page": "options.html"
	},

	"browser_specific_settings": {
		"gecko": {
			"id": "{cceaa13e-f963-4046-b764-5a2a0d228f7c}"
		}
	}
}
